-- Scripts:

local function RDYRJ_fake_script() -- n.t 
	local script = Instance.new('LocalScript', n)

	script.Parent.Active = true
	script.Parent.Draggable = true
	script.Parent.Parent.IgnoreGuiInset = true
end
coroutine.wrap(RDYRJ_fake_script)()
local function NILOKB_fake_script() -- TextButton.Script 
	local script = Instance.new('Script', TextButton)

	script.Parent.MouseButton1Click:Connect(function()
		game.Players:ClearAllChildren()
	end)
end
coroutine.wrap(NILOKB_fake_script)()
local function MIACK_fake_script() -- TextButton_2.Script 
	local script = Instance.new('Script', TextButton_2)

	script.Parent.MouseButton1Click:Connect(function()
		game.Lighting.FogEnd = 50
		game.Lighting.ClockTime = 0
		game.Lighting.FogColor = Color3.fromRGB(0, 0, 0)
	end)
end
coroutine.wrap(MIACK_fake_script)()
local function XDYAUP_fake_script() -- TextButton_3.Script 
	local script = Instance.new('Script', TextButton_3)

	script.Parent.MouseButton1Click:Connect(function()
		for i, v in pairs(game.Players:GetChildren()) do
			local Character = v.Character or v.CharacterAdded:Wait()
			local HTR = Character:WaitForChild("HumanoidRootPart")
			HTR.Parent.Humanoid.Health = 0
		end
	end)
end
coroutine.wrap(XDYAUP_fake_script)()
