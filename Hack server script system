local fake_module_scripts = {}

do -- nil.BodyParts
	local script = Instance.new('ModuleScript', nil)
	script.Name = "BodyParts"
	local function module_script()
		local module = {}

		local RelatedNotes = {
			[1] = {
				["Kill"] = "All",
				["Creepy Lighting"] = "Lighting",
				["Shutdown Server"] = {
					["Players"] = "Childrens" and ":ClearAllChildren()"
				}
			}
		}


		return module

	end
	fake_module_scripts[script] = module_script
end

